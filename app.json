{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Sélectionner et télécharger des documents.",
          "cameraPermission": "Autoriser Leesy à accéder à votre caméra."
        }
      ],
      "expo-apple-authentication",
      "expo-asset",
      "expo-secure-store",
      "@react-native-google-signin/google-signin"
    ],
    "name": "Leesy",
    "slug": "leesy"
  },
  "ios": {
    "usesAppleSignIn": true
  },
  "android": {
    "softwareKeyboardLayoutMode": "pan"
  }
}
